{
  "name": "next-ts-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "test": "jest --passWithNoTests",
    "dev": "next dev --turbopack",
    "prebuild": "pnpm install",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint -c ./eslint.config.mjs './**/*.{ts,tsx}'",
    "lint-fix": "eslint -c ./eslint.config.mjs './**/*.{ts,tsx}' --fix",
    "clean": "rimraf build",
    "pretest": "pnpm build",
    "all": "pnpm install && pnpm lint && pnpm test",
    "postbuild": "pnpm 404",
    "post404": "cp public/favicon.ico out",
    "prestyle-check": "pnpm install",
    "preformat": "pnpm install",
    "serve": "http-server out -p 8080"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "@mui/material-nextjs": "^7.0.2",
    "i18next": "^23.16.8",
    "next": "^15.3.0",
    "next-i18next": "^15.4.2",
    "next-language-detector": "^1.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.4.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@mui/icons-material": "^7.0.2",
    "@next/bundle-analyzer": "^15.3.0",
    "@swc/jest": "^0.2.37",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@types/react": "^19.1.1",
    "autoprefixer": "^10.4.21",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "styled-jsx": "^5.1.6",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.6.12"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.0"
    }
  }
}
