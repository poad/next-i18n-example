{
  "name": "next-ts-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "test": "jest --passWithNoTests",
    "dev": "next dev",
    "prebuild": "pnpm install",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint -c ../eslint.config.mjs './**/*.{ts,tsx}'",
    "lint-fix": "eslint -c ../eslint.config.mjs './**/*.{ts,tsx}' --fix",
    "clean": "rimraf build",
    "pretest": "pnpm build",
    "all": "pnpm install && pnpm lint && pnpm test",
    "postbuild": "pnpm 404",
    "post404": "cp public/favicon.ico out",
    "prestyle-check": "pnpm install",
    "preformat": "pnpm install",
    "serve": "http-server out -p 8080"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^5.16.14",
    "i18next": "^23.16.8",
    "next": "^15.1.6",
    "next-i18next": "^15.4.1",
    "next-language-detector": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.16.14",
    "@next/bundle-analyzer": "^14.2.23",
    "@swc/jest": "^0.2.37",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.16",
    "@types/react": "^18.3.18",
    "autoprefixer": "^10.4.20",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "postcss": "^8.5.1",
    "rimraf": "^5.0.10",
    "styled-jsx": "^5.1.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.6.12"
  }
}
